{
  "name": "qoopido.nucleus",
  "title": "Qoopido.nucleus",
  "description": "Lightweight, atomic and modular JavaScript utility library that strongly encourages the creation and usage of small yet flexible, reusable and easily maintainable modules.",
  "version": "1.0.2",
  "homepage": "http://nucleus.qoopido.com",
  "author": {
    "name": "Dirk Lueth",
    "email": "info@qoopido.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dlueth/qoopido.nucleus.git"
  },
  "bugs": {
    "url": "https://github.com/dlueth/qoopido.nucleus/issues"
  },
  "license": "(MIT OR GPL-3.0+)",
  "filename": "base.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/dlueth/qoopido.nucleus.git",
    "basePath": "/dist",
    "files": [
      "**/*.js"
    ]
  },
  "keywords": [
    "atomic",
    "modular",
    "module",
    "library",
    "toolkit",
    "dry",
    "lib",
    "js",
    "javascript",
    "script",
    "lightweight"
  ],
  "devDependencies": {
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-bump": "^1.0.0",
    "gulp-chmod": "^1.2.0",
    "gulp-eslint": "^1.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-livereload": "^3.8.1",
    "gulp-load-plugins": "^1.0.0",
    "gulp-pleeease": "^2.0.1",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.6",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.4"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "no-cond-assign": 0
    },
    "env": {
      "browser": true
    },
    "globals": {
      "demand": true,
      "provide": true,
      "ga": true
    }
  }
}
